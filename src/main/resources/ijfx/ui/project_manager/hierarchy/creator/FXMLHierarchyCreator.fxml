<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="386.0" prefWidth="354.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <bottom>
        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" prefWidth="153.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="setChangeButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="%setChange" />
                <Button fx:id="cancelButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="%cancel" />
