            <VBox alignment="TOP_CENTER" styleClass="dark-background">
               <children>
                      <VBox fx:id="vbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                  </VBox>
                    <Button fx:id="addButton" mnemonicParsing="false" text="Add level">
                        <graphic>
                            <FontAwesomeIconView glyphName="" size="12" textAlignment="CENTER">
                                <styleClass>
                                    <String fx:value="add-icon" />
                                    <String fx:value="big-icon" />
                                </styleClass>
                            </FontAwesomeIconView>
                        </graphic>
                    </Button>
               </children>
            </VBox>
         </content>
        </ScrollPane>
    </center>
