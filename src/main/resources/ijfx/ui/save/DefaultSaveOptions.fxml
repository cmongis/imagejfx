<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="BASELINE_CENTER" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <RadioButton fx:id="replaceFilesBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Replace File" />
      <RadioButton fx:id="newFilesBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Add suffix" />
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="newSuffix" maxWidth="1.7976931348623157E308" promptText="new suffix" />
            <Label fx:id="extension" text=".tiff" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="destinationFolderBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</fx:root>
