<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="BASELINE_CENTER" minHeight="250.0" minWidth="250.0" prefHeight="250.0" prefWidth="250.0" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="title" text="Save Options">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Label>
      <RadioButton fx:id="replaceFilesBtn" mnemonicParsing="false" text="Replace File" />
      <RadioButton fx:id="newFilesBtn" mnemonicParsing="false" text="Add suffix" />
      <HBox alignment="CENTER" prefHeight="39.0" prefWidth="327.0" spacing="5.0">
         <children>
            <TextField fx:id="newSuffix" promptText="new suffix" />
            <Label fx:id="extension" text=".tiff" />
         </children>
      </HBox>
      <Button fx:id="destinationFolderBtn" mnemonicParsing="false" text="Destination folder">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Button>
      <Button fx:id="startBtn" mnemonicParsing="false" text="Start" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
