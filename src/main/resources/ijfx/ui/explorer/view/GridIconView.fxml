<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<fx:root minHeight="-Infinity" minWidth="-Infinity" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ScrollPane fx:id="scrollPane" fitToHeight="false" fitToWidth="false" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="vBox" fillWidth="false" />
         </content>
      </ScrollPane>
   </center>
   <top>
      <GridPane fx:id="topBar" prefHeight="60.0" prefWidth="400.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Rows" />
            <Label text="Columns" GridPane.columnIndex="1" />
            <Label text="Group by" GridPane.columnIndex="2" />
            <ComboBox fx:id="rowsComboBox" prefWidth="150.0" GridPane.rowIndex="1" />
            <ComboBox fx:id="columnsComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ComboBox fx:id="groupComboBox" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <CheckBox fx:id="clusterCheckbox" mnemonicParsing="false" text="Cluster" GridPane.columnIndex="3" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </top>
</fx:root>
